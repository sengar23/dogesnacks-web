<!-- Alert Modal Box-->
<div class="modal fade" id="alert-modal" tabindex="-1" aria-labelledby="alert-modal" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Alert!</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <p class="alert-body-txt">Alert body text goes here.</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="doge-btn back-btn" data-bs-dismiss="modal">Dismiss</button>
            </div>
        </div>
    </div>
</div>




